{
    "name": "jzejda/dalin",
    "type": "project",
    "description": "DaLin Orienteering manage software.",
    "keywords": [
        "laravel",
        "livewire",
        "filamentphp",
        "tailwindcss",
        "orienteering"
    ],
    "license": "MIT",
    "version": "11.1",
    "require": {
        "php": "^8.3",
        "ext-intl": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "bezhansalleh/filament-shield": " ^3.2",
        "cknow/laravel-money": "^v8.1",
        "codeat3/blade-weather-icons": "^1.6",
        "filament/filament": "^3.2",
        "filament/forms": "^3.2",
        "filament/notifications": "^3.2",
        "filament/tables": "^3.2",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/breeze": "^v2.0",
        "laravel/framework": "^11.7",
        "laravel/sanctum": "^v4.0",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^v3.4",
        "maatwebsite/excel": "^3.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "psr/simple-cache": "^2.0",
        "pxlrbt/filament-excel": "^2.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/icalendar-generator": "^2.6",
        "spatie/laravel-markdown": "^2.5",
        "symfony/property-access": "^6.4",
        "symfony/property-info": "^6.4",
        "symfony/serializer": "^6.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^v3.0",
        "fakerphp/faker": "^1.23",
        "filament/upgrade": "^3.2",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.14",
        "laravel/sail": "^v1.29",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
